{
  "manifest_version": 3,
  "name": "__MSG_extensionName__",
  "version": "3.2",
  "description": "__MSG_extensionDescription__",
  "default_locale": "tr",
  "permissions": [
    "sidePanel",
    "scripting",
    "storage",
    "tabs"
  ],
  "host_permissions": [
    "https://urldet.masahin.dev/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "side_panel": {
    "default_path": "sidepanel.html"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "__MSG_panelTitle__",
    "default_icon": {
      "16": "icons/URLDetpng.png",
      "48": "icons/URLDetpng.png",
      "128": "icons/URLDetpng.png"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "*://www.google.com/search*",
        "*://www.google.com.tr/search*"
      ],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "icons/being.png",
        "icons/defacement.png",
        "icons/spam.png",
        "icons/malware.png",
        "icons/phishing.png"
      ],
      "matches": ["<all_urls>"]
    }
  ],
  "icons": {
    "16": "icons/URLDetpng.png",
    "48": "icons/URLDetpng.png",
    "128": "icons/URLDetpng.png"
  }
}